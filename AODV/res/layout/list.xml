<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:padding="8px">
	
	<TextView android:id="@+id/name"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"></TextView>
	<TextView android:id="@+id/position"
		android:layout_marginLeft="6px" android:layout_width="wrap_content"
		android:layout_height="wrap_content" ></TextView>
	<TextView android:id="@+id/number" android:layout_marginLeft="6px"
		android:layout_width="wrap_content" android:layout_height="wrap_content" ></TextView>
</LinearLayout>
